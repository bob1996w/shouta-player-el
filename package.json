{
  "name": "shouta-player-el",
  "version": "0.0.1",
  "description": "A music player based on Electron",
  "main": "./dist/server/electron.js",
  "scripts": {
    "build:server": "tsc -p tsconfig.server.json",
    "build:audio": "webpack",
    "clean:server": "rm -rf ./dist",
    "clean:audio": "rm -rf ./static/js",
    "watch": "tsc -p tsconfig.server.json -w",
    "build": "run-s build:server build:audio",
    "clean": "run-s clean:server clean:audio",
    "start": "npm run clean && npm run build && electron ."
  },
  "author": "bob1996w",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.2",
    "electron": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "express": "^4.17.1",
    "howler": "^2.1.3",
    "music-metadata": "^6.3.1",
    "portfinder": "^1.0.25"
  },
  "files": []
}
